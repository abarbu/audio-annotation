    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "concurrently \"yarn run start-redis\" \"yarn run start-server\" \"yarn run start-client\" \"yarn run start-telemetry-server\"",
    "start-telemetry-server": "npx supervisor --no-restart-on-err -w server.ts telemetry-server.js",
    "start-server": "npx supervisor --no-restart-on-err -w server.ts server.ts",
    "start-redis": "redis-stable/src/redis-server redis.conf",
    "start-client": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "install": "wget -N http://download.redis.io/redis-stable.tar.gz -O redis-stable.tar.gz && tar xvzf redis-stable.tar.gz && (cd redis-stable; make)",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "semi": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^5.3.18",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-cssresources": "^5.3.18",
